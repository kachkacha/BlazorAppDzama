@rendermode InteractiveServer


<RadzenStack class="w-100 rz-text-align-center site-header" Orientation="Orientation.Horizontal" Gap="0">
    <RadzenColumn Style="width:40%;">
        <RadzenLink class="text-white fw-bolder h6 mb-0" Text="Projects" />
    </RadzenColumn>

    <RadzenColumn Style="width:55%;">
        <RadzenStack Orientation="Orientation.Horizontal" AlignItems="AlignItems.Center" JustifyContent="JustifyContent.SpaceAround" Wrap="FlexWrap.NoWrap" Gap="1rem">
            <RadzenStack Orientation="Orientation.Horizontal" AlignItems="AlignItems.Center">
                <RadzenLink class="text-white fw-bolder h6 mb-0 link-underline-warning" Path="#about-us" Text="About Us" />
                <RadzenLink class="text-white fw-bolder h6 mb-0" Path="#" Text="Reviews" />
                <RadzenLink class="text-white fw-bolder h6 mb-0" Path="#" Text="Gallery" />
                <RadzenLink class="text-white fw-bolder h6 mb-0" Path="#" Text="Blog" />
                <RadzenLink class="text-white fw-bolder h6 mb-0" Path="#" Text="Contact" />
            </RadzenStack>
            <RadzenStack Orientation="Orientation.Horizontal" AlignItems="AlignItems.Center" Gap="0.5rem">
                <RadzenLink >
                    <i class="fa-brands fa-facebook-f text-white" style="width:14;height:14"></i>
                </RadzenLink>
                <RadzenLink >
                    <i class="fa-brands fa-instagram text-white" style="width:14;height:14"></i>
                </RadzenLink>
            </RadzenStack>
            <RadzenStack Orientation="Orientation.Horizontal" AlignItems="AlignItems.Center" Gap="0.25rem">
                <RadzenLink class="text-white fw-bolder h6 mb-0" Text="En" />
                <RadzenText class="text-white fw-bolder h6 mb-0" Text="|" />
                <RadzenLink class="text-white fw-bolder h6 mb-0" Text="Ge" />
            </RadzenStack>
        </RadzenStack>

    </RadzenColumn>
    
</RadzenStack>

